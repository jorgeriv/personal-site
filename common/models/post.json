{
  "name": "Post",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "content": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "language": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "tags": {
      "type": [
        "string"
      ],
      "index": true
    },
    "title": {
      "type": "string",
      "required": true,
      "index": true
    },
    "category": {
      "type": "string",
      "required": true,
      "index": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
